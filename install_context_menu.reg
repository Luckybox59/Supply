Windows Registry Editor Version 5.00

; Context menu ONLY on folder background: right click on empty space -> Run ParserGUI here
[HKEY_CLASSES_ROOT\Directory\Background\shell\Run_ParserGUI]
@="Run ParserGUI here"
"Icon"="pythonw.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\Run_ParserGUI\command]
@="cmd.exe /c start \"\" /D \"%V\" pythonw \"D:\\Program files\\myProjects\\Parser\\gui.py\""
